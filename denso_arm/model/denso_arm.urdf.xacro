<?xml version="1.0" ?>
<robot
<xmlns:xacro="http://www.ros.org/wiki/xacro"
       name="denso_arm">

  <material name="Grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="LightGrey">
    <color rgba="0.6 0.6 0.6 1.0"/>
  </material>

  <link name="base">
    <visual>
      <origin xyz="0 0 0.098" rpy="0 0 0" />
      <geometry name="base_visual">
	<box size="0.195 0.195 0.195" />
      </geometry>
      <material name="Grey" />
    </visual>
  </link>

