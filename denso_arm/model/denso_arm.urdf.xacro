      <material name="Grey" />
    </visual>
  </link>

  <joint name="denso_J0" type="revolute">
    <parent link="base"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.198" rpy="0 0 1.57" />
    <axis xyz="0 0 1" />
    <limit lower="-2.967" upper="2.967" effort="10"
